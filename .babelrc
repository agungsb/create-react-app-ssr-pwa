{
  "presets": [
    "env",
    "react-app",
    "es2015"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "transform-class-properties",
    [
      "import-inspector",
      {
        "serverSideRequirePath": true,
        "webpackRequireWeakId": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-remove-console"
      ]
    },
    "development": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./config/webpack.config.dev.js"
          }
        ],
        [
          "import-inspector",
          {
            "serverSideRequirePath": true,
            "webpackRequireWeakId": true
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./config/webpack.config.prod.js"
          }
        ],
        [
          "import-inspector",
          {
            "serverSideRequirePath": true,
            "webpackRequireWeakId": true
          }
        ]
      ]
    }
  }
}