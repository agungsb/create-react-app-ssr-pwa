{
  "presets": [
    "env",
    "react-app",
    "es2015"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "transform-class-properties",
    "syntax-dynamic-import",
    ["import-inspector", {
      "serverSideRequirePath": true
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-remove-console"
      ]
    },
    "development": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./config/webpack.config.dev.js"
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./config/webpack.config.prod.js"
          }
        ]
      ]
    }
  }
}