{
  "presets": [
    "env",
    "react-app",
    "es2015"
  ],
  "plugins": [
    "add-module-exports",
    "transform-runtime",
    "transform-class-properties"
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./config/webpack.config.dev.js"
          }
        ]
      ]
    },
    "production": {
      "plugins": [
        [
          "babel-plugin-webpack-alias",
          {
            "config": "./config/webpack.config.prod.js"
          }
        ],
        "transform-remove-console"
      ]
    }
  }
}